{
  "presets": [
    ["@vue/app"],
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "debug": true,
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8", "ie >= 11"]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import"
  ]
}
